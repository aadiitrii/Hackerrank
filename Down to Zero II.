#include <stdio.h>
#include <stdlib.h>
#include <limits.h>

#define MAX 1000001

int moves[MAX];

void precompute() {
    for (int i = 0; i < MAX; i++) moves[i] = INT_MAX;
    moves[0] = 0;
    for (int i = 1; i < MAX; i++) {
        if (moves[i] > moves[i - 1] + 1) moves[i] = moves[i - 1] + 1;
        for (int j = 2; j * j <= i; j++) {
            if (i % j == 0) {
                int a = j, b = i / j;
                if (moves[i] > moves[a] + 1) moves[i] = moves[a] + 1;
                if (moves[i] > moves[b] + 1) moves[i] = moves[b] + 1;
            }
        }
    }
}

int main() {
    precompute();
    int q, n;
    scanf("%d", &q);
    while (q--) {
        scanf("%d", &n);
        printf("%d\n", moves[n]);
    }
    return 0;
}
