#include <stdio.h>
#include <stdlib.h>

int main() {
    int n, m;
    scanf("%d", &n);

    int ranked[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &ranked[i]);
    }

    scanf("%d", &m);
    int player[m];
    for (int i = 0; i < m; i++) {
        scanf("%d", &player[i]);
    }

    int *unique = malloc(n * sizeof(int));
    int size = 0;
    unique[size++] = ranked[0];
    for (int i = 1; i < n; i++) {
        if (ranked[i] != ranked[i - 1]) {
            unique[size++] = ranked[i];
        }
    }

    int index = size - 1;
    for (int i = 0; i < m; i++) {
        while (index >= 0 && player[i] >= unique[index]) {
            index--;
        }
        printf("%d\n", index + 2);
    }

    free(unique);
    return 0;
}
